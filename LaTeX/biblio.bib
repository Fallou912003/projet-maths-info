@InProceedings{NuskenZiegler2004,
author="N{\"u}sken, Michael
and Ziegler, Martin",
editor="Albers, Susanne
and Radzik, Tomasz",
title="Fast Multipoint Evaluation of Bivariate Polynomials",
booktitle="Algorithms -- ESA 2004",
year="2004",
publisher="Springer Berlin Heidelberg",
address="Berlin, Heidelberg",
pages="544--555",
abstract="We generalize univariate multipoint evaluation of polynomials of degree n at sublinear amortized cost per point. More precisely, it is shown how to evaluate a bivariate polynomial p of maximum degree less than n, specified by its n2 coefficients, simultaneously at n2 given points using a total of {\$}{\backslash}mathcal{\{}O{\}}(n^{\{}2.667{\}}){\$}arithmetic operations. In terms of the input size N being quadratic in n, this amounts to an amortized cost of {\$}{\backslash}mathcal{\{}O{\}}(N^{\{}0.334{\}}){\$}per point.",
isbn="978-3-540-30140-0"
}


